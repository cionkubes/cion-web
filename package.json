{
  "name": "cion",
  "version": "0.1.0",
  "description": "Web server for cion",
  "main": "src/web/app.py",
  "scripts": {
    "pack": "webpack --progress --display-error-details",
    "watch": "webpack --watch",
    "prebuild": "cross-env NODE_ENV=production webpack --progress",
    "build:frontend": "docker build -f Dockerfile . --tag tiptk/tipimages:cion-frontend_latest",
    "build:api": "docker build -f ../api/Dockerfile ../api --tag tiptk/tipimages:cion-api_latest",
    "build": "run-p build:*",
    "test": "babel-node --presets node6 src/tests/run.js"
  },
  "repository": "https://github.com/haraldfw/quak.git",
